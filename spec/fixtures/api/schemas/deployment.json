{
  "type": "object",
  "required": [
    "sha",
    "created_at",
    "deployed_at",
    "iid",
    "tag",
    "last?",
    "ref",
    "id"
  ],
  "properties": {
    "created_at": { "type": "string" },
    "deployed_at": { "type": ["string", "null"] },
    "id": { "type": "integer" },
    "iid": { "type": "integer" },
    "last?": { "type": "boolean" },
    "ref": {
      "type": "object",
      "required": [
        "name"
      ],
      "properties": {
        "name": { "type": "string" },
        "ref_path": { "type": "string" }
      },
      "additionalProperties": false
    },
    "sha": { "type": "string" },
    "tag": { "type": "boolean" },
    "user": {
      "oneOf": [
        { "type": "null" },
        { "$ref": "entities/user.json" }
      ]
    },
    "commit": {
      "oneOf": [
        { "type": "null" },
        { "$ref": "entities/commit.json" }
      ]
    },
    "deployable": {
      "oneOf": [
        { "type": "null" },
        { "$ref": "job/job.json" }
      ]
    },
    "cluster": {
      "oneOf": [
        { "type": "null" },
        { "$ref": "cluster_basic.json" }
      ]
    },
    "manual_actions": {
      "type": "array",
      "items": { "$ref": "job/job.json" }
    },
    "scheduled_actions": {
      "type": "array",
      "items": { "$ref": "job/job.json" }
    },
    "status": { "type": "string" }
  },
  "additionalProperties": false
}
