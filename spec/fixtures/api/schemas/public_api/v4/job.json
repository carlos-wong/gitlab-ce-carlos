{
  "type": "object",
  "required": [
    "id",
    "status",
    "stage",
    "name",
    "ref",
    "tag",
    "coverage",
    "created_at",
    "started_at",
    "finished_at",
    "duration",
    "user",
    "commit",
    "pipeline",
    "web_url",
    "artifacts",
    "artifacts_expire_at",
    "runner"
  ],
  "properties": {
    "id": { "type": "integer" },
    "status": { "type": "string" },
    "stage": { "type": "string" },
    "name": { "type": "string" },
    "ref": { "type": "string" },
    "tag": { "type": "boolean" },
    "coverage": { "type": ["number", "null"] },
    "created_at": { "type": "string" },
    "started_at": { "type": ["null", "string"] },
    "finished_at": { "type": ["null", "string"] },
    "duration": { "type": ["null", "number"] },
    "user": { "$ref": "user/basic.json" },
    "commit": {
      "oneOf": [
        { "type": "null" },
        { "$ref": "commit/basic.json" }
      ]
    },
    "pipeline": { "$ref": "pipeline/basic.json" },
    "web_url": { "type": "string" },
    "artifacts": {
      "type": "array",
      "items": { "$ref": "artifact.json" }
    },
    "artifacts_file": {
      "oneOf": [
        { "type": "null" },
        { "$ref": "artifact_file.json" }
      ]
    },
    "artifacts_expire_at": { "type": ["null", "string"] },
    "runner": {
      "oneOf": [
        { "type": "null" },
        { "$ref": "runner.json" }
      ]
    }
  },
  "additionalProperties":false
}
